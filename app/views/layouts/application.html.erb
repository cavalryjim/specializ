<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag 'jquery-ui-1.8.16.custom.min', 'jquery.rating', 'jquery.dataTables.min', 'jquery.multiselect.min', 'autocomplete-rails'  %>

  
</head>
<body>
    <div class="container">
      <%= render 'layouts/header' %>
	  
      <section class="round">

        <% flash.each do |key, value| %>
          <div class="flash <%= key %>"><%= value %></div>
        <% end %>

        <%= yield %>

      </section>
      <%= render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
    </div>
</body>
</html>